FACTOR -> NUM
TERM1 -> #
TERM -> FACTOR TERM1
ARITH1 -> #
ARITHLOGIC -> TERM ARITH1
EXPR -> ARITHLOGIC
FACTOR -> NUM
TERM1 -> #
TERM -> FACTOR TERM1
ARITH1 -> #
ARITHLOGIC -> TERM ARITH1
EXPR -> ARITHLOGIC
PRIMITIVE -> #
PRIMITIVE -> ID '=' EXPR NEWLINE PRIMITIVE
PRIMITIVE -> ID '=' EXPR NEWLINE PRIMITIVE
FACTOR -> ID
TERM1 -> #
TERM -> FACTOR TERM1
FACTOR -> ID
TERM1 -> #
TERM -> FACTOR TERM1
ARITH1 -> #
ARITH1 -> '+' TERM ARITH1
ARITHLOGIC -> TERM ARITH1
EXPR -> ARITHLOGIC
STATEMENT -> ID '=' EXPR
BODY -> STATEMENT
FUNCTION -> '{' ID BODY '}'
FUNCTIONS -> #
FUNCTIONS -> FUNCTION NEWLINE FUNCTIONS
FACTOR -> NUM
TERM1 -> #
TERM -> FACTOR TERM1
ARITH1 -> #
ARITHLOGIC -> TERM ARITH1
EXPR -> ARITHLOGIC
PARAM -> ID '=' EXPR
PARAMLIST -> #
ENGINE -> '[' PLAY WITH PARAM PARAMLIST ']'
START  ->  SECTION1 NEWLINE PRIMITIVE SECTION2 NEWLINE FUNCTIONS SECTION3 NEWLINE ENGINE
